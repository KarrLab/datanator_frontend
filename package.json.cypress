    "@fortawesome/react-fontawesome": "^0.1.8",
    "@material-ui/core": "^4.9.2",
    "axios": "^0.19.2",
    "babel-plugin-root-import": "^6.4.1",
    "chart.js": "^2.9.3",
    "chartjs-chart-box-and-violin-plot": "^2.2.0",
    "customize-cra": "^0.9.1",
    "history": "^4.10.1",
    "mathjs": "^6.6.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-app-rewired": "^2.1.5",
    "react-dom": "^16.12.0",
