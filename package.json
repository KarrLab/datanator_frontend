    "ag-grid-enterprise": "^22.1.1",
    "ag-grid-react": "^22.1.1",
    "antd": "^3.26.4",
    "axios": "^0.19.0",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-root-import": "^6.4.1",
    "chart.js": "^2.9.3",
    "chartjs-chart-box-and-violin-plot": "^2.2.0",
    "customize-cra": "^0.9.1",
    "node-sass": "^4.13.0",
    "react": "^16.12.0",
    "react-app-rewired": "^2.1.5",
    "react-dom": "^16.12.0",
