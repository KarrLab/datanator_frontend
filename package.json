{
  "name": "datanator_frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ag-grid-community/react": "^22.1.2",
    "@ag-grid-enterprise/all-modules": "^22.1.2",
    "@blueprintjs/core": "^3.23.1",
    "@blueprintjs/select": "^3.11.2",
    "@bruit/component": "^1.3.13",
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@material-ui/core": "^4.9.2",
    "axios": "^0.19.2",
    "babel-plugin-root-import": "^6.4.1",
    "chart.js": "^2.9.3",
    "chartjs-chart-box-and-violin-plot": "^2.2.0",
    "customize-cra": "^0.9.1",
    "history": "^4.10.1",
    "mathjs": "^6.6.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-app-rewired": "^2.1.5",
    "react-dom": "^16.12.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-router-hash-link": "^1.2.2",
    "react-scripts": "^3.3.1",
    "react-social-icons": "^4.1.0",
    "react-string-replace": "^0.4.4",
    "sprintf-js": "^1.1.2"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^1.11.1",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-istanbul": "^6.0.0",
    "core-js": "^3.6.4",
    "coveralls": "^3.0.9",
    "cypress": "^4.0.1",
    "depcheck": "^0.9.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "http": "0.0.0",
    "istanbul-lib-coverage": "^3.0.0",
    "jest": "^24.9.0",
    "jest-junit": "^10.0.0",
    "jsonlint": "^1.6.3",
    "lcov-result-merger": "^3.1.0",
    "mocha-junit-reporter": "^1.23.3",
    "node-sass": "^4.13.1",
    "nyc": "^15.0.0",
    "prettier": "^1.19.1",
    "regenerator-runtime": "^0.13.3",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "stylelint": "^13.1.0",
    "stylelint-config-standard": "^20.0.0",
    "svglint": "^1.0.5",
    "svgo": "^1.3.2",
    "tldrlegal": "^1.0.11",
    "wait-on": "^4.0.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "stop": "node scripts/stop.js",
    "build": "react-app-rewired build",
    "test-unit": "jest",
    "test-unit-coverage": "jest --coverage",
    "test-integration-run": "cypress run",
    "test-integration-open": "cypress open",
    "test-integration-report": "nyc report --reporter=text-summary --reporter=html",
    "eject": "react-scripts eject",
    "clean-heroku": "scripts/clean-heroku.sh"
  },
  "eslintConfig": {
    "ignorePatterns": [
      "build/",
      "coverage/",
      "node_modules/",
      "scripts/",
      "test-results",
      "config-overrides.js"
    ],
    "plugins": [
      "react",
      "prettier"
    ],
    "parser": "babel-eslint",
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "rules": {
      "prettier/prettier": "error"
    },
    "settings": {
      "react": {
        "createClass": "createReactClass",
        "pragma": "React",
        "version": "detect",
        "flowVersion": "0.53"
      },
      "propWrapperFunctions": [
        "forbidExtraProps",
        {
          "property": "freeze",
          "object": "Object"
        },
        {
          "property": "myFavoriteWrapper"
        }
      ],
      "linkComponents": [
        "Hyperlink",
        {
          "name": "Link",
          "linkAttribute": "to"
        }
      ]
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "no-descending-specificity": null,
      "font-family-no-missing-generic-family-keyword": null,
      "no-duplicate-selectors": null
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "babel-plugin-root-import",
        {
          "rootPathSuffix": "./src",
          "rootPathPrefix": "~"
        }
      ],
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ]
    ]
  },
  "jest": {
    "rootDir": ".",
    "modulePaths": [
      "<rootDir>/src"
    ],
    "roots": [
      "<rootDir>/src/__tests__"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testEnvironment": "jsdom",
    "reporters": [
      "default",
      "jest-junit"
    ],
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/coverage/",
      "<rootDir>/integration-tests/",
      "<rootDir>/node_modules/",
      "<rootDir>/scripts/",
      "<rootDir>/src/__tests__",
      "<rootDir>/test-results/"
    ],
    "coverageDirectory": "coverage/unit-tests",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover"
    ]
  },
  "jest-junit": {
    "outputDirectory": "test-results/unit-tests/",
    "outputName": "results.xml"
  },
  "nyc": {
    "report-dir": "coverage/integration-tests"
  }
}
